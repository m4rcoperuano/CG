@model IEnumerable<CodeGeneration.Domain.Models.LanguagesModel>

@{
    ViewBag.Title = "All LAnguages";
}

<div class="container">
    <div class="row">
        <div class="col-md-12">
            <table class="table table-striped table-hover">
                <thead>
                    <tr>
                        <th>CG Name
                        </th>
                        <th>Description
                        </th>
                        <th>Last Updated
                        </th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var lm in Model)
                    {
                        <tr>
                            <td>
                                <a href="@Url.Action("Details", "CodeGeneration", new { @id = lm.CG_Id })">
                                    @lm.CGName
                                </a>
                            </td>
                            <td>
                                @lm.Description
                            </td>
                            <td>
                                @lm.LastUpdated
                            </td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>
